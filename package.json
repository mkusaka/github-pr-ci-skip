{
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@types/chrome": "^0.1.4",
    "@types/webextension-polyfill": "^0.12.3",
    "@vitest/ui": "^3.2.4",
    "@webext-core/fake-browser": "^1.3.2",
    "esbuild": "^0.25.9",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "oxlint": "^1.13.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vite": "^5.4.19",
    "vitest": "^3.2.4",
    "webextension-polyfill": "^0.12.0"
  },
  "name": "github-pr-ci-skip",
  "version": "0.0.5",
  "main": "index.js",
  "repository": "https://github.com/mkusaka/github-pr-ci-skip.git",
  "author": "mkusaka <hinoshita1992@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "esbuild content.ts --bundle --target=chrome100 --outfile=content.js",
    "watch": "esbuild content.ts --bundle --target=chrome100 --watch --outfile=content.js",
    "typecheck": "tsc --noEmit",
    "lint": "oxlint .",
    "lint:fix": "oxlint . --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "package": "rm -f package.zip && pnpm build && zip -r package.zip . -x '.git/**' './node_modules/**' 'pnpm-lock.yaml' '*.ts' '.idea/**' 'tsconfig.json' 'package.json' '.gitignore'"
  }
}
