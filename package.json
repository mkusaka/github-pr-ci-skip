{
  "devDependencies": {
    "@crxjs/vite-plugin": "2.3.0",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/user-event": "14.6.1",
    "@types/chrome": "0.1.36",
    "@vitest/ui": "4.0.18",
    "@webext-core/fake-browser": "1.3.4",
    "happy-dom": "20.5.0",
    "oxlint": "1.43.0",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "4.0.18"
  },
  "name": "github-pr-ci-skip",
  "version": "0.0.13",
  "main": "index.js",
  "repository": "https://github.com/mkusaka/github-pr-ci-skip.git",
  "author": "mkusaka <hinoshita1992@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "oxlint . --import-plugin --tsconfig ./tsconfig.json",
    "lint:fix": "oxlint . --import-plugin --tsconfig ./tsconfig.json --fix",
    "lint:ci": "oxlint . --import-plugin --tsconfig ./tsconfig.json --deny-warnings",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "package": "rm -f package.zip && pnpm build && cd dist && zip -r ../package.zip ."
  }
}
